def plus_five(num: int) -> int:
    return num + 5 #return is not a function do not add brackets

def main():
    result = plus_five(10)
    print(result)

if __name__ == "__main__":
    main()

# ***Pure Functions***
# 3 rules of creating functions:
# Take arguments
# Return a value
# No print/input
#     Try to keep and inputs and prining in the main function

def count_nums(nums: list[int]) -> int:
    evens = 0
    for num in nums:
        if num % 2 == 0:
            evens += 1

    return evens

result = count_nums([2, 1, 2, 3, 4])
print(result)
# 6.4
# Varibales outside a function are global, variables inside are local
mark = 49
def automatic_100():
    global mark
    mark = 100

print(mark)

if __name__ == "__main__":
    automatic_100()
print(f"Thew new mark is {mark}")

