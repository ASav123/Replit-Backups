# Homework
numbers = []
for _ in range(10):
    numbers.append(random.randrange(1, 51))
print(numbers)

num_find = int(input("What number to find: "))
count = 0
for n in numbers:
    if n == num_find:
        print(f"{num_find} is in the list")
        count += 1
    else:
        print(f"{num_find} is not in the list")

print(f"{num_find} was found {count} times")



num_find = int(input("What number to find: "))
for i, num in enumerate(numbers):
    if num_find == num:
        print(f"{num_find} was found in index {i}")


marks = [86, 48, 78, 10, 60, 30]
for i, mark in enumerate(marks):
    if mark < 50:
        marks[i] = 50
print(marks)


numbers = []
for _ in range(10):
    numbers.append(random.randrange(0, 101, 2))
print(numbers)
location = None
high = 0
for i, num in enumerate(numbers):
    if num > high:
        high = num
        location = i
print(f" The highest number is {high} at index: {location}")

