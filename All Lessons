    if count3 % 3 == 0 and count3 % 5 != 0:
        total3 += count3
    count3 += 1

print(total3)

# 4.3 (input in a loop)

nums = int(input("How many numbers do you have?: "))
count3 = 0
total3 = 0
while count3 < nums:
    num = int(input("Enter a number: "))
    count3 += 1
    total3 += num
print(total3)

losses = 0
wins = 0
count4 = 1
while count4 <= 6:
    score = input(f"did the team [w]in or [l]ose? in round {count4}: ").lower()
    if score == "w":
        wins += 1
    elif score == "l":
        losses += 1
    else:
        print("Invalid Option!")
        continue
    count4 += 1


ratio = wins / losses
print(f" your win/loss ratio is {round(ratio,1)}%")


# 4.4 (break and continue)

n = 5
while n > 0:
    n -= 1
    if n == 2:
        continue
        # This makes the code go right back to the top of the loop
    print(n)
print("Loop is finished.")


import random

# 4.5 (Post-Test Loops)
num = 7
while True:
    print(num)
    num += 1
    if num > 9:
        break

while True:
    coin = random.randrange(2)
    choice = input("Whould you like to flip a coin?(y/n):").lower()
    if choice == "n":
        break    
    elif choice == "y":
        if coin == 0:
            print("Tails!")
        elif coin == 1:
            print("Heads!")
    else:
        print("Invalid option.")
        continue
print("Loop broken")


