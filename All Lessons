# 3.
numbers_words = "one,two,three,four"
print(numbers_words)
numbers = []
for number in numbers_words.split(","):
    if number.lower() == "zero":
        numbers.append(0)
    elif number.lower() == "one":
        numbers.append(1)
    elif number.lower() == "two":
        numbers.append(2)
    elif number.lower() == "three":
        numbers.append(3)
    elif number.lower() == "four":
        numbers.append(4)
    elif number.lower() == "five":
        numbers.append(5)
print(numbers)
# 4.
games = "W W L T T W"
games_list = []
games_list = games.split(" ")
print(games_list)
score = []
for outcome in games_list:
    print(outcome)
    if outcome.upper() == "W":
        score.append(2)
    elif outcome == "L":
        score.append(0)
    elif outcome == "T":
        score.append(1)
print(score)

# 5.
x_coords = "x:3,x:6,x:14,x:22"
x_values = x_coords.split(",")
print(x_values)
x = []
for value in x_values:
    specif_value = value.split(":")
    print(specif_value)
    x.append(int(specif_value[1]))
print(x)

# 6.
# coords_list = "x:2,y:5 - x:5,y:11 - x:7,y:14"
# test = []
# final = []
# x_y = cordinates.split(" - ")
# print(x_y)   
# for set in x_y:
#     x_y = set.split(",")
#     print(x_y)
#     for set in x_y:
#         test.append(int(set[2:]))
#         if len(test) == 2:
#             final.append(test)
#             test = []  
# print(final)
coords_string = "x:2,y:5 - x:5,y:11 - x:7,y:14"
coords_list = []
for coords in coords_string.split(" - "):
    coords_final = []
    coords_cut = coords.split(",")

    coords_x = coords_cut[0]
    x_split = coords_x.split(":")
    x_value = int(x_split[1])

    coords_y = coords_cut[1]
    y_split = coords_y.split(":")
    y_value = int(y_split[1])


    coords_final.append(x_value)
    coords_final.append(y_value)
    print(coords_final)

    coords_list.append(coords_final)
print(coords_list)







