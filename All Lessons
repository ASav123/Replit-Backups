for x in range(-10, 11): 
    if x == 10: 
        print(f"{x}     {x**2}") 
    else: 
        print(f"{x}   {x**2}")
        print(f"{x + 0.5}  {(x + 0.5)**2}")
        # Use this if counting by halves

for x in range(1, 21):
    if x % 2 == 0:
        print(f"{x} <")
    else:
        print(x)

# 4.7 

x = 0
name = "Dave"
for char in  name:
    if char == "a":
        print(f"An \"a\" has been detected! at index {x}")
    print(char)
    x += 1

for x in range(len(name)):
    char = name[x]
    if char == "a":
        print(f"An 'a' has been detected! at index {x}")
    print(char)

new_string = ""
for char in name:
        # if char == "a" or char == "e" or char == "i" or char == "o" or char == "u":
        if char in "aeiou":
            new_string += "-"
        else:
            new_string += char 
print(new_string)

