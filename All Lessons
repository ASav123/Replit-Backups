def add(a: int, b: int): # a and b are the peramiters, they defince what is required
    print(f"a is given as {a}")
    print(f"b is given as {b}")
    total = a + b
    print(total)

if __name__ == "__main__":
    add(6, 7) #theses are the argumets that which are given to the function

