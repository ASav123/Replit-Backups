    print(f"Hello {name}")

if __name__ == "__main__":
    name = input("Enter name: ")
    hello_name(name)

# 3. Create a function to take a number and RETURN its square.
def square(num: float) -> float:
    diffrent_num = num ** 2
    return diffrent_num

if __name__ == "__main__":
    num = float(input("Enter a number: "))
    square_number = square(num)
    print(f"The square is {square_number}")
# 4. Create a function to take two numbers and RETURN their sum.
def sum(num1: float, num2: float) -> float:
    final_sum = num1 + num2
    return final_sum

if __name__ == "__main__":
    numbers = input("Enter two numbers seperated by a space")
    numbers_split = numbers.split(" ")
    final_sum = sum(float(numbers_split[0]), float(numbers_split[1]))
    print(f"The sum is {final_sum}")
# 5. Create a function to take a first and a last name and RETURN it in the format "Last, First".
def format_name(first: str, last: str) -> str:
    name_final = f"{last}, {first}"
    return name_final

if __name__ == "__main__":
    first = input("Enter a first name: ")
    last = input("Enter a last name: ")
    final_name = format_name(first, last)
    print(f"Name formated: {final_name}")
