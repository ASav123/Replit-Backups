def main():
    print("Welcome to the chair calcualtor.")
    tables = int(input("Enter tables: "))
    chairs_per_table = int(input("Enter chairs per table: "))
    chairs = tables * chairs_per_table
    print(f"You will need {chairs} chairs.")

if __name__ == "__main__":
    main()

