# Expections:


while True:
    try:
        print("[1] Attack")
        print("[2] Do Nothing \n")
        choice = int(input("Enter some numbers: "))
        if choice not in [1, 2]:
            print("Test")
            raise ValueError("Invalid Menu Option!")

    except ValueError:
        print("Invlaid Input!")

    if choice == 1:
        print("You Attack!")
    elif choice == 2:
        print("You Do Nothing!")
    else:
        break
        # To make code below it readable



